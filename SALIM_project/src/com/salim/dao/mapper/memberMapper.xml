<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" > 
<mapper namespace= "memberModule.member"> 
 
 <!-- 회원가입 insert -->
 <insert id="insertMember" parameterType="member">
 	insert into member values( #{memberId}, #{name}, #{age}, #{birthday},#{password},#{email})
 </insert>
 <!-- 회원탈퇴 delete -->
 <delete id="deleteMember" parameterType="string">
 	delete from member 
 	where member_Id=#{value}
 </delete>
 <!-- 회원정보 수정 update -->
<!--  <update id="updateMember" parameterType="member"> 
 	update member
 	set
 		member_Id=#{memberId}
 </update>	 -->
 
 <select id ="selectMemberById" parameterType="string" resultType="member">
 	
 	select member_id memberId, name, age, birthday, password, eamil 
 	from member
 	where member_id=#{value}
 	
 </select>
 
 
</mapper> 